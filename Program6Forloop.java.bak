class Program6Forloop 
{
	public static void main(String[] args) 
	{
		System.out.println("Hello World!");
		
		for (char ch='A';ch<='Z' ;ch++ )
		
		{
			System.out.println(ch);
		}
	System.out.println("no World!");
	}
}
